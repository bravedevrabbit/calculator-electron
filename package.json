{
  "name": "elcalc",
  "productName": "elcalc",
  "website": "https://github.com/elcalc/elcalc/releases/latest",
  "version": "4.0.2",
  "files": [
    "**/*",
    "build/icon.*",
    "src/*",
    "src/assets/css/*",
    "src/assets/js/*"
  ],
  "description": "Simple, Cross-Platform calculator built with Electron.",
  "main": "src/main.js",
  "electronVersion": "2.0.2",
  "keywords": [
    "electron",
    "calculator",
    "app",
    "simple",
    "calculate"
  ],
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "dist": "electron-builder --linux deb rpm AppImage --x64 --ia32 --windows portable --x64 --ia32 --mac dmg"
  },
  "author": "Antoni Kepinski <hello@akepinski.me> (https://akepinski.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elcalc/elcalc.git"
  },
  "bugs": {
    "url": "https://github.com/elcalc/elcalc/issues"
  },
  "devDependencies": {
    "electron": "2.0.2",
    "electron-builder": "20.15.1"
  },
  "dependencies": {
    "ajv": "^6.5.1"
  },
  "build": {
    "appId": "me.akepinski.elcalc",
    "copyright": "Copyright Â© 2018 Antoni Kepinski"
  }
}
